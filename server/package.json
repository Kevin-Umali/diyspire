{
  "name": "make-me-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "start:dev": "ts-node src/index.ts",
    "watch:dev": "nodemon -L",
    "prebuild": "npm install && npm audit fix",
    "build": "npx prisma generate && tsc -p .",
    "docker:up": "docker-compose -f docker-compose.dev.yml up",
    "docker-prod:up": "docker-compose -f docker-compose.yml up",
    "docker:build": "docker-compose -f docker-compose.dev.yml build",
    "docker-prod:build": "docker-compose -f docker-compose.yml build",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker-prod:down": "docker-compose -f docker-compose.yml down",
    "lint": "eslint \"src/**/*.ts\" --quiet --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "npm run format && npm run lint",
    "test": "NODE_ENV=test jest --detectOpenHandles"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "apicache": "^1.6.3",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "isbot": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.6.0",
    "ua-parser-js": "^1.0.36",
    "unsplash-js": "^7.0.18",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.2",
    "@types/apicache": "^1.6.2",
    "@types/bcrypt": "^5.0.1",
    "@types/compression": "^1.7.3",
    "@types/cookie-parser": "^1.4.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.37",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/memory-cache": "^0.2.3",
    "@types/node": "^20.6.1",
    "@types/qs": "^6.9.8",
    "@types/supertest": "^2.0.12",
    "@types/ua-parser-js": "^0.7.38",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "prisma": "^5.3.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": false,
    "printWidth": 200,
    "tabWidth": 2,
    "endOfLine": "auto"
  }
}
