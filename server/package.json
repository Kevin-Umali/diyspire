{
  "name": "make-me-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "start:dev": "ts-node src/index.ts",
    "watch:dev": "nodemon -L",
    "prebuild": "npm install && npm audit fix",
    "build": "npx prisma generate && tsc -p .",
    "docker:up": "docker-compose -f docker-compose.dev.yml up",
    "docker-prod:up": "docker-compose -f docker-compose.yml up",
    "docker:build": "docker-compose -f docker-compose.dev.yml build",
    "docker-prod:build": "docker-compose -f docker-compose.yml build",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker-prod:down": "docker-compose -f docker-compose.yml down",
    "lint": "eslint \"src/**/*.ts\" --quiet --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "npm run format && npm run lint",
    "test": "NODE_ENV=test jest --detectOpenHandles"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "@supercharge/promise-pool": "^3.1.1",
    "apicache": "^1.6.3",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "isbot": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.12",
    "openai": "^4.28.4",
    "ua-parser-js": "^1.0.37",
    "unsplash-js": "^7.0.19",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.2",
    "@types/apicache": "^1.6.6",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.43",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/memory-cache": "^0.2.5",
    "@types/node": "^20.11.25",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/qs": "^6.9.12",
    "@types/supertest": "^6.0.2",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "prettier": {
    "semi": true,
