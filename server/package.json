{
  "name": "make-me-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "start:dev": "node -r @swc/register src/index.ts",
    "watch:dev": "nodemon -L",
    "copy-files": "copyfiles -u 1 src/template/*.html dist/src/",
    "prebuild": "npm install && npm audit fix",
    "build": "npx prisma generate && swc ./src ./prisma/seed.ts -d dist --copy-files",
    "build:tsc": "npx prisma generate && tsc -p . && npm run copy-files",
    "prisma:migrate": "bash prisma.sh",
    "docker:up": "docker-compose -f docker-compose.dev.yml up",
    "docker-prod:up": "docker-compose -f docker-compose.yml up",
    "docker:build": "docker-compose -f docker-compose.dev.yml build --no-cache",
    "docker-prod:build": "docker-compose -f docker-compose.yml build",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker-prod:down": "docker-compose -f docker-compose.yml down",
    "docker:prune": "docker container prune -f && docker image prune -a -f && docker volume prune -f",
    "lint": "eslint \"src/**/*.ts\" --quiet --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "npm run format && npm run lint",
    "test": "NODE_ENV=test jest --detectOpenHandles"
  },
  "prisma": {
    "seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "@supercharge/promise-pool": "^3.1.1",
    "@swc/helpers": "^0.5.6",
    "apicache": "^1.6.3",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.7.8",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "isbot": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.12",
    "openai": "^4.28.4",
    "pgvector": "^0.1.8",
    "rate-limiter-flexible": "^5.0.0",
    "ua-parser-js": "^1.0.37",
    "unsplash-js": "^7.0.19",
    "winston": "^3.12.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.4.8",
    "@swc/register": "^0.1.10",
    "@types/apicache": "^1.6.6",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.43",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/memory-cache": "^0.2.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.5",
    "@types/nodemailer": "^6.4.14",
    "@types/qs": "^6.9.12",
    "@types/supertest": "^6.0.2",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": false,
    "printWidth": 200,
    "tabWidth": 2,
    "endOfLine": "auto",
    "importOrder": [
      "^node:(.*)$",
      "^[a-z-0-9]+$",
      "^(express|cors|helmet|compression|dotenv|cookie-parser)$",
      "<THIRD_PARTY_MODULES>",
      "^(prisma|@prisma/client)$",
      "^(@?\\w*/middleware/.*|express-rate-limit|cors|helmet|compression|cookie-parser)$",
      "^@?\\w*/(services|libs)/.*$",
      "^@?\\w*/(routes|controllers)/.*$",
      "^@?\\w*/(models|interfaces|types)/.*$",
      "^[./]"
    ],
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports"
    ]
  }
}
